<?xml version="1.0" ?>
<openerp>
        <record id="view_company_form_fiscalcode" model="ir.ui.view">
            <field name="name">res.company.fiscalcode</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">         	
                <xpath expr="//field[@name='vat']" position="after">
                   <field name="fiscalcode"/>
                   <field name="is_soletrader"/>
                </xpath>
            </field>
        </record>
</openerp>
