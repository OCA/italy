<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <template id="account_invoice_print_incoterm"
                    inherit_id="account.report_invoice_document">
            <xpath expr="//div[@name='invoice_address']" position="after">
                <div name="invoice_address_supplier" class="col-xs-5 col-xs-offset-7" t-if="o.rc_partner_supplier_id">
                    <br/>
                    <address t-field="o.rc_partner_supplier_id"
                        t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />
                    <span t-if="o.rc_partner_supplier_id.vat">TIN: <span t-field="o.rc_partner_supplier_id.vat"/></span>
                </div>
            </xpath>
        </template>

    </data>
</openerp>